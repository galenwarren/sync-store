{
  "name": "sync-store",
  "version": "0.0.1",
  "description": "Utilities to sync databases to local stores",
  "main": "src/index.js",
  "files": [
    "./dist"
  ],
  "directories": {
    "src": "./src",
    "dist": "./dist",
    "docs": "./docs"
  },
  "scripts": {
    "commitmsg": "commitlint -E GIT_PARAMS",
    "precommit": "lint-staged",
    "lint": "eslint --fix $npm_package_directories_src",
    "test": "jest $npm_package_directories_src",
    "test:watch": "jest --watch $npm_package_directories_src",
    "coverage": "jest --coverage $npm_package_directories_src",
    "coverage:record": "jest --coverage $npm_package_directories_src --coverageReporters=text-lcov | coveralls",
    "build": "babel $npm_package_directories_src --out-dir $npm_package_directories_dist",
    "docs:dev": "vuepress dev $npm_package_directories_docs",
    "docs:build": "vuepress build $npm_package_directories_docs",
    "commit": "git-cz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/galenwarren/sync-store.git"
  },
  "keywords": [
    "sync",
    "store",
    "vue",
    "vuex",
    "firebase"
  ],
  "author": "Galen Warren <galenwarren@users.noreply.github.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/galenwarren/sync-store/issues"
  },
  "homepage": "https://github.com/galenwarren/sync-store#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "coveralls": "^3.0.2",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.3.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.18.0",
    "eslint-plugin-prettier": "^2.6.2",
    "husky": "^0.14.3",
    "jest": "^23.4.2",
    "lint-staged": "^7.2.0",
    "prettier": "^1.14.0",
    "vuepress": "^0.13.1"
  },
  "dependencies": {
    "firebase": "^5.3.1",
    "memoizee": "^0.4.12",
    "object-path": "^0.11.4",
    "vue": "^2.5.17"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
